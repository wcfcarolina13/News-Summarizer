{
  "name": "CryptoSum Newsletter",
  "description": "Custom extraction rules for CryptoSum beehiiv newsletter - focused on market/trading news",

  "include_patterns": [],

  "exclude_patterns": [
    "subscribe",
    "unsubscribe",
    "share this",
    "refer a friend",
    "advertisement",
    "sponsored",
    "prediction market",
    "polymarket",
    "kalshi",
    "predictit",
    "regulation",
    "regulatory",
    "regulator",
    "SEC ",
    "CFTC",
    "DOJ",
    "indictment",
    "sentenced",
    "hack",
    "hacked",
    "exploit",
    "exploited",
    "breach",
    "stolen",
    "theft",
    "scam",
    "fraud",
    "arrest",
    "charged",
    "lawsuit"
  ],

  "exclude_categories": [
    "Regulatory",
    "Legal",
    "Enforcement",
    "Legal & Enforcement"
  ],

  "allowed_domains": [],

  "blocked_domains": [
    "twitter.com",
    "x.com",
    "facebook.com",
    "linkedin.com",
    "instagram.com",
    "beehiiv.com",
    "tiktok.com",
    "polymarket.com",
    "kalshi.com"
  ],

  "csv_columns": [
    "title",
    "url",
    "category",
    "source_name",
    "date_published"
  ],

  "notes": "Extracts market/trading news from CryptoSum. Filters out: prediction markets (ads), regulatory news, hacks/exploits, legal/enforcement news."
}
